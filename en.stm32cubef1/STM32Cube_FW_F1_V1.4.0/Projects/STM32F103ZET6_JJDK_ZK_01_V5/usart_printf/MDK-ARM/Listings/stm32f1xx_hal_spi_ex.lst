N  */
N#define __HAL_SPI_CLEAR_MODFFLAG(__HANDLE__)                      \
Ndo{                                                               \
N    __IO uint32_t tmpreg;                                         \
N    tmpreg = (__HANDLE__)->Instance->SR;                          \
